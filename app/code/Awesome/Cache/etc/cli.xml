<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="../../Console/Schema/console_command.xsd">
    <namespace name="cache">
        <command name="clean" class="Awesome\Cache\Console\Clean">
            <description><![CDATA[Flush XML, including CLI and page cache.]]></description>
        </command>
    </namespace>
    <namespace name="static">
        <command name="deploy" class="Awesome\Cache\Console\StaticDeploy">
            <description><![CDATA[Generate static files (assets).]]></description>
        </command>
    </namespace>
</config>
