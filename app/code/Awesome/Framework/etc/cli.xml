<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../Console/Schema/cli.xsd">
    <namespace name="maintenance">
        <command name="enable" class="Awesome\Framework\Console\MaintenanceEnable"/>
        <command name="disable" class="Awesome\Framework\Console\MaintenanceDisable"/>
        <command name="status" class="Awesome\Framework\Console\MaintenanceStatus"/>
    </namespace>
    <namespace name="config">
        <command name="get" class="Awesome\Framework\Console\ConfigGet"/>
        <command name="set" class="Awesome\Framework\Console\ConfigSet"/>
    </namespace>
</config>
