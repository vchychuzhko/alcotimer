#!/usr/bin/env php
<?php

if (PHP_SAPI !== 'cli') {
    echo 'bin/console must be run as a CLI application';
    exit(1);
}

require_once(__DIR__ . '/../app/autoload.php');

try {
    $console = new \Awesome\Console\Model\Console();
    $console->run();
} catch (\Throwable $t) {
    echo 'Error occurred: ' . $t->getMessage();
    exit(1);
}
